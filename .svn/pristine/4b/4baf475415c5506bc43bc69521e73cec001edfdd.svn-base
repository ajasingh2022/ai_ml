package com.capgemini.sesp.ast.android.ui.wo.bean;

public class EvChargerListBean {
    private Integer index;
    private String power;
    private String currentType;

    public EvChargerListBean(String power, String currentType, Integer index) {
        this.power = power;
        this.index = index;
        this.currentType = currentType;
    }

    public EvChargerListBean() {

    }

    public Integer getIndex() {
        return index;
    }

    public void setIndex(Integer index) {
        this.index = index;
    }

    public String getCode() {
        return index + "_" + currentType.toUpperCase() + "_EV";
    }

    public String getPower() {
        return power;
    }

    public void setPower(String power) {
        this.power = power;
    }

    public String getCurrentType() {
        return currentType;
    }

    public void setCurrentType(String currentType) {
        this.currentType = currentType;
    }
}
