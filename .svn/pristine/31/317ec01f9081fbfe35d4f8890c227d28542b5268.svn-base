<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/order_list_row"
    android:layout_width="match_parent"
    android:layout_height="45dip"
    android:background="@color/colorDetailView"
    android:gravity="center_horizontal"
    android:orientation="horizontal">


    <ImageView
        android:id="@+id/time_reservation"
        android:layout_width="15dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_weight="0.8"
        android:contentDescription="@string/time_reservation"
        android:src="@drawable/ic_time_reservation" />

    <ImageView
        android:id="@+id/started"
        android:layout_width="15dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_weight="0.8"
        android:contentDescription="@string/start"
        android:src="@drawable/ic_wo_started" />

    <ImageView
        android:id="@+id/assign"
        android:layout_width="15dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_weight="0.8"
        android:contentDescription="@string/assign"
        android:src="@drawable/ic_wo_assigned_user" />

    <ImageView
        android:id="@+id/key"
        android:layout_width="15dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_weight="0.8"
        android:contentDescription="@string/key_info"
        android:src="@drawable/ic_key" />

    <TextView
        android:id="@+id/priority_type"
        android:layout_width="15dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:layout_gravity="center_vertical"
        android:layout_weight="0.8"
        android:textColor="@color/colorBlack"
        android:textSize="@dimen/standard_text"
        android:textStyle="bold" />
    <TextView
        android:id="@+id/inst_code"
        android:layout_width="160dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center|start"
        android:layout_weight="1"
        android:textAlignment="viewStart"
        android:textSize="@dimen/standard_text" />

    <ImageView android:id="@+id/iv_arrow_expand"
        android:layout_width="15dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_weight="0.8"
        android:src="@drawable/selector_arrow_down"/>

</LinearLayout>