<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true"
    android:orientation="vertical">

    <TextView
        android:id="@+id/techAccessTxLbl"
        android:layout_width="match_parent"
        android:padding="@dimen/screen_padding"
        android:layout_height="wrap_content"
        android:theme="@style/SESPHeading"
        android:text="@string/accessibility_for_technicians" />

    <!--  <TextView
          android:id="@+id/techAccessTxValNew"
          android:layout_width="match_parent"
          android:layout_height="wrap_content" />
  -->
    <RadioGroup
        android:id="@+id/techAccessRadioGroup"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/control_margin"
        android:orientation="horizontal"
        android:padding="@dimen/screen_padding">

        <!-- android:background="@drawable/ic_border_shadow">-->

        <RadioButton
            android:id="@+id/techAccessRadioBtnYes"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="@string/accessible"
            android:textColor="@color/colorBlack"
            android:theme="@style/SespRadioButton" />

        <RadioButton
            android:id="@+id/techAccessRadioBtnNo"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="@string/notaccessible"
            android:textColor="@color/colorBlack"
            android:theme="@style/SespRadioButton" />
    </RadioGroup>


    <!-- layout for holding button for accessibility for technician -->
